<% provide(:header_title, "お知らせ一覧") %>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% if @reports.present? %>
      <% @reports.each do |report| %>
        <tr>
          <td><%= report.title %></td>
        </tr>
      <% end %>
    <% else %>
      <div class="">
        お知らせはありません
      </div>
    <% end %>
  </tbody>
</table>
